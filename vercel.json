{
"routes": [
  {
    "src": "/static/(.*)",           
    "dest": "/static/$1"
  },
  {
    "src": "/assets/(.*)",           
    "dest": "/assets/$1"
  },
  {
    "src": "/(.*\\.js)",
    "dest": "/$1"
  },
  {
    "src": "/(.*\\.css)",
    "dest": "/$1"
  },
  {
    "src": "/(.*\\.ico)",
    "dest": "/$1"
  },
  {
    "src": "/(.*\\.png)",
    "dest": "/$1"
  },
  {
    "src": "/editor/(?<roomId>[^/]+)",
    "dest": "/lottery.html"
  },
  {
    "src": "/login",
    "dest": "/lottery.html"
  },
  {
    "src": "/register",
    "dest": "/lottery.html"
  },
  {
    "src": "/createRoom",
    "dest": "/lottery.html"
  },
  {
    "src": "/",
    "dest": "/lottery.html"
  },
  {
    "src": "/.*",
    "dest": "/lottery.html"
  }
]
}